    "id": "4_2_74",
    "verses": {
        "number": 74,
        "name": "سورة المدثر",
        "englishName": "Al-Muddaththir",
        "englishNameTranslation": "The Cloaked One",
        "revelationType": "Meccan",
        "numberOfAyahs": 56,
        "ayahs": [
            {
                "number": 5496,
                "text": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ يَا أَيُّهَا الْمُدَّثِّرُ",
                "numberInSurah": 1,
                "juz": 29,
                "manzil": 7,
                "page": 575,
                "ruku": 510,
                "hizbQuarter": 230,
                "sajda": false
            },
            {
                "number": 5497,
                "text": "قُمْ فَأَنْذِرْ",
                "numberInSurah": 2,
                "juz": 29,
                "manzil": 7,
                "page": 575,
                "ruku": 510,
                "hizbQuarter": 230,
                "sajda": false
            },
            {
                "number": 5498,
                "text": "وَرَبَّكَ فَكَبِّرْ",
                "numberInSurah": 3,
                "juz": 29,
                "manzil": 7,
                "page": 575,
                "ruku": 510,
                "hizbQuarter": 230,
                "sajda": false
            },
            {
                "number": 5499,
                "text": "وَثِيَابَكَ فَطَهِّرْ",
                "numberInSurah": 4,
                "juz": 29,
                "manzil": 7,
                "page": 575,
                "ruku": 510,
                "hizbQuarter": 230,
                "sajda": false
            },
            {
                "number": 5500,
                "text": "وَالرُّجْزَ فَاهْجُرْ",
                "numberInSurah": 5,
                "juz": 29,
                "manzil": 7,
                "page": 575,
                "ruku": 510,
                "hizbQuarter": 230,
                "sajda": false
            },
            {
                "number": 5501,
                "text": "وَلَا تَمْنُنْ تَسْتَكْثِرُ",
                "numberInSurah": 6,
                "juz": 29,
                "manzil": 7,
                "page": 575,
                "ruku": 510,
                "hizbQuarter": 230,
                "sajda": false
            },
            {
                "number": 5502,
                "text": "وَلِرَبِّكَ فَاصْبِرْ",
                "numberInSurah": 7,
                "juz": 29,
                "manzil": 7,
                "page": 575,
                "ruku": 510,
                "hizbQuarter": 230,
                "sajda": false
            },
            {
                "number": 5503,
                "text": "فَإِذَا نُقِرَ فِي النَّاقُورِ",
                "numberInSurah": 8,
                "juz": 29,
                "manzil": 7,
                "page": 575,
                "ruku": 510,
                "hizbQuarter": 230,
                "sajda": false
            },
            {
                "number": 5504,
                "text": "فَذَٰلِكَ يَوْمَئِذٍ يَوْمٌ عَسِيرٌ",
                "numberInSurah": 9,
                "juz": 29,
                "manzil": 7,
                "page": 575,
                "ruku": 510,
                "hizbQuarter": 230,
                "sajda": false
            },
            {
                "number": 5505,
                "text": "عَلَى الْكَافِرِينَ غَيْرُ يَسِيرٍ",
                "numberInSurah": 10,
                "juz": 29,
                "manzil": 7,
                "page": 575,
                "ruku": 510,
                "hizbQuarter": 230,
                "sajda": false
            },
            {
                "number": 5506,
                "text": "ذَرْنِي وَمَنْ خَلَقْتُ وَحِيدًا",
                "numberInSurah": 11,
                "juz": 29,
                "manzil": 7,
                "page": 575,
                "ruku": 510,
                "hizbQuarter": 230,
                "sajda": false
            },
            {
                "number": 5507,
                "text": "وَجَعَلْتُ لَهُ مَالًا مَمْدُودًا",
                "numberInSurah": 12,
                "juz": 29,
                "manzil": 7,
                "page": 575,
                "ruku": 510,
                "hizbQuarter": 230,
                "sajda": false
            },
            {
                "number": 5508,
                "text": "وَبَنِينَ شُهُودًا",
                "numberInSurah": 13,
                "juz": 29,
                "manzil": 7,
                "page": 575,
                "ruku": 510,
                "hizbQuarter": 230,
                "sajda": false
            },
            {
                "number": 5509,
                "text": "وَمَهَّدْتُ لَهُ تَمْهِيدًا",
                "numberInSurah": 14,
                "juz": 29,
                "manzil": 7,
                "page": 575,
                "ruku": 510,
                "hizbQuarter": 230,
                "sajda": false
            },
            {
                "number": 5510,
                "text": "ثُمَّ يَطْمَعُ أَنْ أَزِيدَ",
                "numberInSurah": 15,
                "juz": 29,
                "manzil": 7,
                "page": 575,
                "ruku": 510,
                "hizbQuarter": 230,
                "sajda": false
            },
            {
                "number": 5511,
                "text": "كَلَّا ۖ إِنَّهُ كَانَ لِآيَاتِنَا عَنِيدًا",
                "numberInSurah": 16,
                "juz": 29,
                "manzil": 7,
                "page": 575,
                "ruku": 510,
                "hizbQuarter": 230,
                "sajda": false
            },
            {
                "number": 5512,
                "text": "سَأُرْهِقُهُ صَعُودًا",
                "numberInSurah": 17,
                "juz": 29,
                "manzil": 7,
                "page": 575,
                "ruku": 510,
                "hizbQuarter": 230,
                "sajda": false
            },
            {
                "number": 5513,
                "text": "إِنَّهُ فَكَّرَ وَقَدَّرَ",
                "numberInSurah": 18,
                "juz": 29,
                "manzil": 7,
                "page": 576,
                "ruku": 510,
                "hizbQuarter": 230,
                "sajda": false
            },
            {
                "number": 5514,
                "text": "فَقُتِلَ كَيْفَ قَدَّرَ",
                "numberInSurah": 19,
                "juz": 29,
                "manzil": 7,
                "page": 576,
                "ruku": 510,
                "hizbQuarter": 230,
                "sajda": false
            },
            {
                "number": 5515,
                "text": "ثُمَّ قُتِلَ كَيْفَ قَدَّرَ",
                "numberInSurah": 20,
                "juz": 29,
                "manzil": 7,
                "page": 576,
                "ruku": 510,
                "hizbQuarter": 230,
