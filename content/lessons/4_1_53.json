    "id": "4_1_53",
    "verses": {
        "number": 53,
        "name": "سورة النجم",
        "englishName": "An-Najm",
        "englishNameTranslation": "The Star",
        "revelationType": "Meccan",
        "numberOfAyahs": 62,
        "ayahs": [
            {
                "number": 4785,
                "text": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ وَالنَّجْمِ إِذَا هَوَىٰ",
                "numberInSurah": 1,
                "juz": 27,
                "manzil": 7,
                "page": 526,
                "ruku": 460,
                "hizbQuarter": 210,
                "sajda": false
            },
            {
                "number": 4786,
                "text": "مَا ضَلَّ صَاحِبُكُمْ وَمَا غَوَىٰ",
                "numberInSurah": 2,
                "juz": 27,
                "manzil": 7,
                "page": 526,
                "ruku": 460,
                "hizbQuarter": 210,
                "sajda": false
            },
            {
                "number": 4787,
                "text": "وَمَا يَنْطِقُ عَنِ الْهَوَىٰ",
                "numberInSurah": 3,
                "juz": 27,
                "manzil": 7,
                "page": 526,
                "ruku": 460,
                "hizbQuarter": 210,
                "sajda": false
            },
            {
                "number": 4788,
                "text": "إِنْ هُوَ إِلَّا وَحْيٌ يُوحَىٰ",
                "numberInSurah": 4,
                "juz": 27,
                "manzil": 7,
                "page": 526,
                "ruku": 460,
                "hizbQuarter": 210,
                "sajda": false
            },
            {
                "number": 4789,
                "text": "عَلَّمَهُ شَدِيدُ الْقُوَىٰ",
                "numberInSurah": 5,
                "juz": 27,
                "manzil": 7,
                "page": 526,
                "ruku": 460,
                "hizbQuarter": 210,
                "sajda": false
            },
            {
                "number": 4790,
                "text": "ذُو مِرَّةٍ فَاسْتَوَىٰ",
                "numberInSurah": 6,
                "juz": 27,
                "manzil": 7,
                "page": 526,
                "ruku": 460,
                "hizbQuarter": 210,
                "sajda": false
            },
            {
                "number": 4791,
                "text": "وَهُوَ بِالْأُفُقِ الْأَعْلَىٰ",
                "numberInSurah": 7,
                "juz": 27,
                "manzil": 7,
                "page": 526,
                "ruku": 460,
                "hizbQuarter": 210,
                "sajda": false
            },
            {
                "number": 4792,
                "text": "ثُمَّ دَنَا فَتَدَلَّىٰ",
                "numberInSurah": 8,
                "juz": 27,
                "manzil": 7,
                "page": 526,
                "ruku": 460,
                "hizbQuarter": 210,
                "sajda": false
            },
            {
                "number": 4793,
                "text": "فَكَانَ قَابَ قَوْسَيْنِ أَوْ أَدْنَىٰ",
                "numberInSurah": 9,
                "juz": 27,
