    "title": "سورة الجن تفسير النسفي",
    "nav": "",
    "id": "4_1_72",
    "verses": {
        "number": 72,
        "name": "سورة الجن",
        "englishName": "Al-Jinn",
        "englishNameTranslation": "The Jinn",
        "revelationType": "Meccan",
        "numberOfAyahs": 28,
        "ayahs": [
            {
                "number": 5448,
                "text": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ قُلْ أُوحِيَ إِلَيَّ أَنَّهُ اسْتَمَعَ نَفَرٌ مِنَ الْجِنِّ فَقَالُوا إِنَّا سَمِعْنَا قُرْآنًا عَجَبًا",
                "numberInSurah": 1,
                "juz": 29,
                "manzil": 7,
                "page": 572,
                "ruku": 506,
                "hizbQuarter": 229,
                "sajda": false
            },
            {
                "number": 5449,
                "text": "يَهْدِي إِلَى الرُّشْدِ فَآمَنَّا بِهِ ۖ وَلَنْ نُشْرِكَ بِرَبِّنَا أَحَدًا",
                "numberInSurah": 2,
                "juz": 29,
                "manzil": 7,
                "page": 572,
                "ruku": 506,
                "hizbQuarter": 229,
                "sajda": false
            },
            {
                "number": 5450,
                "text": "وَأَنَّهُ تَعَالَىٰ جَدُّ رَبِّنَا مَا اتَّخَذَ صَاحِبَةً وَلَا وَلَدًا",
                "numberInSurah": 3,
                "juz": 29,
                "manzil": 7,
                "page": 572,
                "ruku": 506,
                "hizbQuarter": 229,
                "sajda": false
            },
            {
                "number": 5451,
                "text": "وَأَنَّهُ كَانَ يَقُولُ سَفِيهُنَا عَلَى اللَّهِ شَطَطًا",
                "numberInSurah": 4,
                "juz": 29,
                "manzil": 7,
                "page": 572,
                "ruku": 506,
                "hizbQuarter": 229,
                "sajda": false
            },
            {
                "number": 5452,
                "text": "وَأَنَّا ظَنَنَّا أَنْ لَنْ تَقُولَ الْإِنْسُ وَالْجِنُّ عَلَى اللَّهِ كَذِبًا",
                "numberInSurah": 5,
                "juz": 29,
                "manzil": 7,
                "page": 572,
                "ruku": 506,
                "hizbQuarter": 229,
                "sajda": false
            },
            {
                "number": 5453,
                "text": "وَأَنَّهُ كَانَ رِجَالٌ مِنَ الْإِنْسِ يَعُوذُونَ بِرِجَالٍ مِنَ الْجِنِّ فَزَادُوهُمْ رَهَقًا",
                "numberInSurah": 6,
                "juz": 29,
                "manzil": 7,
                "page": 572,
                "ruku": 506,
                "hizbQuarter": 229,
                "sajda": false
            },
            {
                "number": 5454,
                "text": "وَأَنَّهُمْ ظَنُّوا كَمَا ظَنَنْتُمْ أَنْ لَنْ يَبْعَثَ اللَّهُ أَحَدًا",
                "numberInSurah": 7,
                "juz": 29,
                "manzil": 7,
                "page": 572,
                "ruku": 506,
                "hizbQuarter": 229,
                "sajda": false
            },
            {
                "number": 5455,
                "text": "وَأَنَّا لَمَسْنَا السَّمَاءَ فَوَجَدْنَاهَا مُلِئَتْ حَرَسًا شَدِيدًا وَشُهُبًا",
                "numberInSurah": 8,
                "juz": 29,
                "manzil": 7,
                "page": 572,
                "ruku": 506,
                "hizbQuarter": 229,
                "sajda": false
            },
            {
                "number": 5456,
                "text": "وَأَنَّا كُنَّا نَقْعُدُ مِنْهَا مَقَاعِدَ لِلسَّمْعِ ۖ فَمَنْ يَسْتَمِعِ الْآنَ يَجِدْ لَهُ شِهَابًا رَصَدًا",
                "numberInSurah": 9,
                "juz": 29,
                "manzil": 7,
                "page": 572,
                "ruku": 506,
                "hizbQuarter": 229,
                "sajda": false
            },
            {
                "number": 5457,
                "text": "وَأَنَّا لَا نَدْرِي أَشَرٌّ أُرِيدَ بِمَنْ فِي الْأَرْضِ أَمْ أَرَادَ بِهِمْ رَبُّهُمْ رَشَدًا",
                "numberInSurah": 10,
                "juz": 29,
                "manzil": 7,
                "page": 572,
                "ruku": 506,
                "hizbQuarter": 229,
                "sajda": false
            },
            {
                "number": 5458,
                "text": "وَأَنَّا مِنَّا الصَّالِحُونَ وَمِنَّا دُونَ ذَٰلِكَ ۖ كُنَّا طَرَائِقَ قِدَدًا",
                "numberInSurah": 11,
                "juz": 29,
                "manzil": 7,
                "page": 572,
                "ruku": 506,
                "hizbQuarter": 229,
                "sajda": false
            },
            {
                "number": 5459,
                "text": "وَأَنَّا ظَنَنَّا أَنْ لَنْ نُعْجِزَ اللَّهَ فِي الْأَرْضِ وَلَنْ نُعْجِزَهُ هَرَبًا",
                "numberInSurah": 12,
                "juz": 29,
                "manzil": 7,
                "page": 572,
                "ruku": 506,
                "hizbQuarter": 229,
                "sajda": false
            },
            {
                "number": 5460,
                "text": "وَأَنَّا لَمَّا سَمِعْنَا الْهُدَىٰ آمَنَّا بِهِ ۖ فَمَنْ يُؤْمِنْ بِرَبِّهِ فَلَا يَخَافُ بَخْسًا وَلَا رَهَقًا",
                "numberInSurah": 13,
                "juz": 29,
                "manzil": 7,
                "page": 572,
                "ruku": 506,
                "hizbQuarter": 229,
                "sajda": false
            },
            {
                "number": 5461,
                "text": "وَأَنَّا مِنَّا الْمُسْلِمُونَ وَمِنَّا الْقَاسِطُونَ ۖ فَمَنْ أَسْلَمَ فَأُولَٰئِكَ تَحَرَّوْا رَشَدًا",
                "numberInSurah": 14,
                "juz": 29,
                "manzil": 7,
                "page": 573,
                "ruku": 506,
                "hizbQuarter": 229,
                "sajda": false
            },
            {
                "number": 5462,
                "text": "وَأَمَّا الْقَاسِطُونَ فَكَانُوا لِجَهَنَّمَ حَطَبًا",
                "numberInSurah": 15,
                "juz": 29,
                "manzil": 7,
                "page": 573,
                "ruku": 506,
                "hizbQuarter": 229,
                "sajda": false
            },
            {
                "number": 5463,
                "text": "وَأَنْ لَوِ اسْتَقَامُوا عَلَى الطَّرِيقَةِ لَأَسْقَيْنَاهُمْ مَاءً غَدَقًا",
                "numberInSurah": 16,
                "juz": 29,
                "manzil": 7,
                "page": 573,
                "ruku": 506,
                "hizbQuarter": 229,
                "sajda": false
            },
            {
                "number": 5464,
                "text": "لِنَفْتِنَهُمْ فِيهِ ۚ وَمَنْ يُعْرِضْ عَنْ ذِكْرِ رَبِّهِ يَسْلُكْهُ عَذَابًا صَعَدًا",
                "numberInSurah": 17,
                "juz": 29,
                "manzil": 7,
                "page": 573,
                "ruku": 506,
                "hizbQuarter": 229,
                "sajda": false
            },
            {
                "number": 5465,
                "text": "وَأَنَّ الْمَسَاجِدَ لِلَّهِ فَلَا تَدْعُوا مَعَ اللَّهِ أَحَدًا",
                "numberInSurah": 18,
                "juz": 29,
                "manzil": 7,
                "page": 573,
                "ruku": 506,
                "hizbQuarter": 229,
                "sajda": false
            },
            {
                "number": 5466,
