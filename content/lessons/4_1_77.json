    "id": "4_1_77",
    "verses": {
        "number": 77,
        "name": "سورة المرسلات",
        "englishName": "Al-Mursalaat",
        "englishNameTranslation": "The Emissaries",
        "revelationType": "Meccan",
        "numberOfAyahs": 50,
        "ayahs": [
            {
                "number": 5623,
                "text": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ وَالْمُرْسَلَاتِ عُرْفًا",
                "numberInSurah": 1,
                "juz": 29,
                "manzil": 7,
                "page": 580,
                "ruku": 516,
                "hizbQuarter": 232,
                "sajda": false
            },
            {
                "number": 5624,
                "text": "فَالْعَاصِفَاتِ عَصْفًا",
                "numberInSurah": 2,
                "juz": 29,
                "manzil": 7,
                "page": 580,
                "ruku": 516,
                "hizbQuarter": 232,
                "sajda": false
            },
            {
                "number": 5625,
                "text": "وَالنَّاشِرَاتِ نَشْرًا",
                "numberInSurah": 3,
                "juz": 29,
                "manzil": 7,
                "page": 580,
                "ruku": 516,
                "hizbQuarter": 232,
                "sajda": false
            },
            {
                "number": 5626,
                "text": "فَالْفَارِقَاتِ فَرْقًا",
                "numberInSurah": 4,
                "juz": 29,
                "manzil": 7,
                "page": 580,
                "ruku": 516,
                "hizbQuarter": 232,
                "sajda": false
            },
            {
                "number": 5627,
                "text": "فَالْمُلْقِيَاتِ ذِكْرًا",
                "numberInSurah": 5,
                "juz": 29,
                "manzil": 7,
                "page": 580,
                "ruku": 516,
                "hizbQuarter": 232,
                "sajda": false
            },
            {
                "number": 5628,
                "text": "عُذْرًا أَوْ نُذْرًا",
                "numberInSurah": 6,
                "juz": 29,
                "manzil": 7,
                "page": 580,
                "ruku": 516,
                "hizbQuarter": 232,
                "sajda": false
            },
            {
                "number": 5629,
                "text": "إِنَّمَا تُوعَدُونَ لَوَاقِعٌ",
                "numberInSurah": 7,
                "juz": 29,
                "manzil": 7,
                "page": 580,
                "ruku": 516,
                "hizbQuarter": 232,
                "sajda": false
            },
            {
                "number": 5630,
                "text": "فَإِذَا النُّجُومُ طُمِسَتْ",
                "numberInSurah": 8,
                "juz": 29,
                "manzil": 7,
                "page": 580,
                "ruku": 516,
                "hizbQuarter": 232,
                "sajda": false
            },
            {
                "number": 5631,
                "text": "وَإِذَا السَّمَاءُ فُرِجَتْ",
                "numberInSurah": 9,
                "juz": 29,
                "manzil": 7,
                "page": 580,
                "ruku": 516,
                "hizbQuarter": 232,
                "sajda": false
            },
            {
                "number": 5632,
                "text": "وَإِذَا الْجِبَالُ نُسِفَتْ",
                "numberInSurah": 10,
                "juz": 29,
                "manzil": 7,
                "page": 580,
                "ruku": 516,
                "hizbQuarter": 232,
                "sajda": false
            },
            {
                "number": 5633,
                "text": "وَإِذَا الرُّسُلُ أُقِّتَتْ",
                "numberInSurah": 11,
                "juz": 29,
                "manzil": 7,
                "page": 580,
                "ruku": 516,
                "hizbQuarter": 232,
                "sajda": false
            },
            {
                "number": 5634,
                "text": "لِأَيِّ يَوْمٍ أُجِّلَتْ",
                "numberInSurah": 12,
                "juz": 29,
                "manzil": 7,
                "page": 580,
                "ruku": 516,
                "hizbQuarter": 232,
                "sajda": false
            },
            {
                "number": 5635,
                "text": "لِيَوْمِ الْفَصْلِ",
                "numberInSurah": 13,
                "juz": 29,
                "manzil": 7,
                "page": 580,
                "ruku": 516,
                "hizbQuarter": 232,
                "sajda": false
            },
            {
                "number": 5636,
                "text": "وَمَا أَدْرَاكَ مَا يَوْمُ الْفَصْلِ",
                "numberInSurah": 14,
                "juz": 29,
                "manzil": 7,
                "page": 580,
                "ruku": 516,
                "hizbQuarter": 232,
                "sajda": false
            },
            {
                "number": 5637,
                "text": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ",
                "numberInSurah": 15,
                "juz": 29,
                "manzil": 7,
                "page": 580,
                "ruku": 516,
                "hizbQuarter": 232,
                "sajda": false
            },
            {
                "number": 5638,
                "text": "أَلَمْ نُهْلِكِ الْأَوَّلِينَ",
                "numberInSurah": 16,
                "juz": 29,
                "manzil": 7,
                "page": 580,
                "ruku": 516,
                "hizbQuarter": 232,
                "sajda": false
            },
            {
                "number": 5639,
                "text": "ثُمَّ نُتْبِعُهُمُ الْآخِرِينَ",
                "numberInSurah": 17,
                "juz": 29,
                "manzil": 7,
                "page": 580,
                "ruku": 516,
                "hizbQuarter": 232,
                "sajda": false
            },
            {
                "number": 5640,
                "text": "كَذَٰلِكَ نَفْعَلُ بِالْمُجْرِمِينَ",
                "numberInSurah": 18,
                "juz": 29,
                "manzil": 7,
                "page": 580,
                "ruku": 516,
                "hizbQuarter": 232,
                "sajda": false
            },
            {
                "number": 5641,
                "text": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ",
                "numberInSurah": 19,
                "juz": 29,
                "manzil": 7,
                "page": 580,
                "ruku": 516,
                "hizbQuarter": 232,
                "sajda": false
            },
            {
                "number": 5642,
                "text": "أَلَمْ نَخْلُقْكُمْ مِنْ مَاءٍ مَهِينٍ",
                "numberInSurah": 20,
                "juz": 29,
                "manzil": 7,
                "page": 581,
                "ruku": 516,
                "hizbQuarter": 232,
                "sajda": false
            },
            {
                "number": 5643,
                "text": "فَجَعَلْنَاهُ فِي قَرَارٍ مَكِينٍ",
                "numberInSurah": 21,
                "juz": 29,
                "manzil": 7,
                "page": 581,
                "ruku": 516,
                "hizbQuarter": 232,
                "sajda": false
            },
            {
                "number": 5644,
                "text": "إِلَىٰ قَدَرٍ مَعْلُومٍ",
                "numberInSurah": 22,
                "juz": 29,
                "manzil": 7,
                "page": 581,
                "ruku": 516,
                "hizbQuarter": 232,
                "sajda": false
            },
            {
                "number": 5645,
                "text": "فَقَدَرْنَا فَنِعْمَ الْقَادِرُونَ",
                "numberInSurah": 23,
                "juz": 29,
                "manzil": 7,
                "page": 581,
                "ruku": 516,
                "hizbQuarter": 232,
                "sajda": false
            },
            {
                "number": 5646,
                "text": "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ",
                "numberInSurah": 24,
                "juz": 29,
                "manzil": 7,
                "page": 581,
                "ruku": 516,
                "hizbQuarter": 232,
                "sajda": false
            },
            {
                "number": 5647,
                "text": "أَلَمْ نَجْعَلِ الْأَرْضَ كِفَاتًا",
                "numberInSurah": 25,
                "juz": 29,
                "manzil": 7,
                "page": 581,
                "ruku": 516,
                "hizbQuarter": 232,
                "sajda": false
            },
            {
                "number": 5648,
                "text": "أَحْيَاءً وَأَمْوَاتًا",
                "numberInSurah": 26,
                "juz": 29,
                "manzil": 7,
                "page": 581,
                "ruku": 516,
                "hizbQuarter": 232,
                "sajda": false
            },
            {
                "number": 5649,
