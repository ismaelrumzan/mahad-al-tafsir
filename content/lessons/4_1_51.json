    "verses": {
        "number": 51,
        "name": "سورة الذاريات",
        "englishName": "Adh-Dhaariyat",
        "englishNameTranslation": "The Winnowing Winds",
        "revelationType": "Meccan",
        "numberOfAyahs": 60,
        "ayahs": [
            {
                "number": 4676,
                "text": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ وَالذَّارِيَاتِ ذَرْوًا",
                "numberInSurah": 1,
                "juz": 26,
                "manzil": 7,
                "page": 520,
                "ruku": 455,
                "hizbQuarter": 208,
                "sajda": false
            },
            {
                "number": 4677,
                "text": "فَالْحَامِلَاتِ وِقْرًا",
                "numberInSurah": 2,
                "juz": 26,
                "manzil": 7,
                "page": 520,
                "ruku": 455,
                "hizbQuarter": 208,
                "sajda": false
            },
            {
                "number": 4678,
                "text": "فَالْجَارِيَاتِ يُسْرًا",
                "numberInSurah": 3,
                "juz": 26,
                "manzil": 7,
                "page": 520,
                "ruku": 455,
                "hizbQuarter": 208,
                "sajda": false
            },
            {
                "number": 4679,
                "text": "فَالْمُقَسِّمَاتِ أَمْرًا",
                "numberInSurah": 4,
                "juz": 26,
                "manzil": 7,
                "page": 520,
                "ruku": 455,
                "hizbQuarter": 208,
                "sajda": false
            },
            {
                "number": 4680,
                "text": "إِنَّمَا تُوعَدُونَ لَصَادِقٌ",
                "numberInSurah": 5,
                "juz": 26,
                "manzil": 7,
                "page": 520,
                "ruku": 455,
                "hizbQuarter": 208,
                "sajda": false
            },
            {
                "number": 4681,
                "text": "وَإِنَّ الدِّينَ لَوَاقِعٌ",
                "numberInSurah": 6,
                "juz": 26,
                "manzil": 7,
                "page": 520,
                "ruku": 455,
                "hizbQuarter": 208,
                "sajda": false
            },
            {
                "number": 4682,
                "text": "وَالسَّمَاءِ ذَاتِ الْحُبُكِ",
                "numberInSurah": 7,
                "juz": 26,
                "manzil": 7,
                "page": 521,
                "ruku": 455,
                "hizbQuarter": 208,
                "sajda": false
            },
            {
                "number": 4683,
                "text": "إِنَّكُمْ لَفِي قَوْلٍ مُخْتَلِفٍ",
                "numberInSurah": 8,
                "juz": 26,
                "manzil": 7,
                "page": 521,
                "ruku": 455,
                "hizbQuarter": 208,
                "sajda": false
            },
            {
                "number": 4684,
                "text": "يُؤْفَكُ عَنْهُ مَنْ أُفِكَ",
                "numberInSurah": 9,
                "juz": 26,
                "manzil": 7,
                "page": 521,
                "ruku": 455,
                "hizbQuarter": 208,
                "sajda": false
            },
            {
                "number": 4685,
                "text": "قُتِلَ الْخَرَّاصُونَ",
                "numberInSurah": 10,
                "juz": 26,
                "manzil": 7,
                "page": 521,
                "ruku": 455,
                "hizbQuarter": 208,
                "sajda": false
            },
            {
                "number": 4686,
                "text": "الَّذِينَ هُمْ فِي غَمْرَةٍ سَاهُونَ",
                "numberInSurah": 11,
                "juz": 26,
                "manzil": 7,
                "page": 521,
                "ruku": 455,
                "hizbQuarter": 208,
                "sajda": false
            },
            {
                "number": 4687,
                "text": "يَسْأَلُونَ أَيَّانَ يَوْمُ الدِّينِ",
                "numberInSurah": 12,
                "juz": 26,
                "manzil": 7,
                "page": 521,
                "ruku": 455,
                "hizbQuarter": 208,
                "sajda": false
            },
            {
                "number": 4688,
                "text": "يَوْمَ هُمْ عَلَى النَّارِ يُفْتَنُونَ",
                "numberInSurah": 13,
                "juz": 26,
                "manzil": 7,
                "page": 521,
                "ruku": 455,
                "hizbQuarter": 208,
                "sajda": false
            },
            {
                "number": 4689,
                "text": "ذُوقُوا فِتْنَتَكُمْ هَٰذَا الَّذِي كُنْتُمْ بِهِ تَسْتَعْجِلُونَ",
                "numberInSurah": 14,
                "juz": 26,
                "manzil": 7,
                "page": 521,
                "ruku": 455,
                "hizbQuarter": 208,
                "sajda": false
            },
            {
                "number": 4690,
                "text": "إِنَّ الْمُتَّقِينَ فِي جَنَّاتٍ وَعُيُونٍ",
                "numberInSurah": 15,
                "juz": 26,
                "manzil": 7,
                "page": 521,
                "ruku": 455,
                "hizbQuarter": 208,
                "sajda": false
            },
            {
                "number": 4691,
                "text": "آخِذِينَ مَا آتَاهُمْ رَبُّهُمْ ۚ إِنَّهُمْ كَانُوا قَبْلَ ذَٰلِكَ مُحْسِنِينَ",
                "numberInSurah": 16,
                "juz": 26,
                "manzil": 7,
                "page": 521,
                "ruku": 455,
                "hizbQuarter": 208,
                "sajda": false
            },
            {
                "number": 4692,
                "text": "كَانُوا قَلِيلًا مِنَ اللَّيْلِ مَا يَهْجَعُونَ",
                "numberInSurah": 17,
                "juz": 26,
                "manzil": 7,
                "page": 521,
                "ruku": 455,
                "hizbQuarter": 208,
                "sajda": false
            },
            {
                "number": 4693,
                "text": "وَبِالْأَسْحَارِ هُمْ يَسْتَغْفِرُونَ",
                "numberInSurah": 18,
                "juz": 26,
                "manzil": 7,
                "page": 521,
                "ruku": 455,
                "hizbQuarter": 208,
                "sajda": false
            },
            {
                "number": 4694,
                "text": "وَفِي أَمْوَالِهِمْ حَقٌّ لِلسَّائِلِ وَالْمَحْرُومِ",
                "numberInSurah": 19,
                "juz": 26,
                "manzil": 7,
                "page": 521,
                "ruku": 455,
                "hizbQuarter": 208,
                "sajda": false
            },
            {
                "number": 4695,
                "text": "وَفِي الْأَرْضِ آيَاتٌ لِلْمُوقِنِينَ",
                "numberInSurah": 20,
                "juz": 26,
                "manzil": 7,
                "page": 521,
                "ruku": 455,
                "hizbQuarter": 208,
                "sajda": false
            },
            {
                "number": 4696,
                "text": "وَفِي أَنْفُسِكُمْ ۚ أَفَلَا تُبْصِرُونَ",
                "numberInSurah": 21,
                "juz": 26,
                "manzil": 7,
                "page": 521,
                "ruku": 455,
                "hizbQuarter": 208,
                "sajda": false
            },
            {
                "number": 4697,
                "text": "وَفِي السَّمَاءِ رِزْقُكُمْ وَمَا تُوعَدُونَ",
                "numberInSurah": 22,
                "juz": 26,
                "manzil": 7,
                "page": 521,
                "ruku": 455,
                "hizbQuarter": 208,
                "sajda": false
            },
            {
                "number": 4698,
                "text": "فَوَرَبِّ السَّمَاءِ وَالْأَرْضِ إِنَّهُ لَحَقٌّ مِثْلَ مَا أَنَّكُمْ تَنْطِقُونَ",
                "numberInSurah": 23,
                "juz": 26,
                "manzil": 7,
                "page": 521,
                "ruku": 455,
                "hizbQuarter": 208,
                "sajda": false
            },
            {
                "number": 4699,
                "text": "هَلْ أَتَاكَ حَدِيثُ ضَيْفِ إِبْرَاهِيمَ الْمُكْرَمِينَ",
                "numberInSurah": 24,
                "juz": 26,
                "manzil": 7,
                "page": 521,
                "ruku": 456,
                "hizbQuarter": 208,
                "sajda": false
            },
            {
                "number": 4700,
                "text": "إِذْ دَخَلُوا عَلَيْهِ فَقَالُوا سَلَامًا ۖ قَالَ سَلَامٌ قَوْمٌ مُنْكَرُونَ",
