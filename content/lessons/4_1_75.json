    "verses": {
        "number": 75,
        "name": "سورة القيامة",
        "englishName": "Al-Qiyaama",
        "englishNameTranslation": "The Resurrection",
        "revelationType": "Meccan",
        "numberOfAyahs": 40,
        "ayahs": [
            {
                "number": 5552,
                "text": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ لَا أُقْسِمُ بِيَوْمِ الْقِيَامَةِ",
                "numberInSurah": 1,
                "juz": 29,
                "manzil": 7,
                "page": 577,
                "ruku": 512,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5553,
                "text": "وَلَا أُقْسِمُ بِالنَّفْسِ اللَّوَّامَةِ",
                "numberInSurah": 2,
                "juz": 29,
                "manzil": 7,
                "page": 577,
                "ruku": 512,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5554,
                "text": "أَيَحْسَبُ الْإِنْسَانُ أَلَّنْ نَجْمَعَ عِظَامَهُ",
                "numberInSurah": 3,
                "juz": 29,
                "manzil": 7,
                "page": 577,
                "ruku": 512,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5555,
                "text": "بَلَىٰ قَادِرِينَ عَلَىٰ أَنْ نُسَوِّيَ بَنَانَهُ",
                "numberInSurah": 4,
                "juz": 29,
                "manzil": 7,
                "page": 577,
                "ruku": 512,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5556,
                "text": "بَلْ يُرِيدُ الْإِنْسَانُ لِيَفْجُرَ أَمَامَهُ",
                "numberInSurah": 5,
                "juz": 29,
                "manzil": 7,
                "page": 577,
                "ruku": 512,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5557,
                "text": "يَسْأَلُ أَيَّانَ يَوْمُ الْقِيَامَةِ",
                "numberInSurah": 6,
                "juz": 29,
                "manzil": 7,
                "page": 577,
                "ruku": 512,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5558,
                "text": "فَإِذَا بَرِقَ الْبَصَرُ",
                "numberInSurah": 7,
                "juz": 29,
                "manzil": 7,
                "page": 577,
                "ruku": 512,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5559,
                "text": "وَخَسَفَ الْقَمَرُ",
                "numberInSurah": 8,
                "juz": 29,
                "manzil": 7,
                "page": 577,
                "ruku": 512,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5560,
                "text": "وَجُمِعَ الشَّمْسُ وَالْقَمَرُ",
                "numberInSurah": 9,
                "juz": 29,
                "manzil": 7,
                "page": 577,
                "ruku": 512,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5561,
                "text": "يَقُولُ الْإِنْسَانُ يَوْمَئِذٍ أَيْنَ الْمَفَرُّ",
                "numberInSurah": 10,
                "juz": 29,
                "manzil": 7,
                "page": 577,
                "ruku": 512,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5562,
                "text": "كَلَّا لَا وَزَرَ",
                "numberInSurah": 11,
                "juz": 29,
                "manzil": 7,
                "page": 577,
                "ruku": 512,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5563,
                "text": "إِلَىٰ رَبِّكَ يَوْمَئِذٍ الْمُسْتَقَرُّ",
                "numberInSurah": 12,
                "juz": 29,
                "manzil": 7,
                "page": 577,
                "ruku": 512,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5564,
                "text": "يُنَبَّأُ الْإِنْسَانُ يَوْمَئِذٍ بِمَا قَدَّمَ وَأَخَّرَ",
                "numberInSurah": 13,
                "juz": 29,
                "manzil": 7,
                "page": 577,
                "ruku": 512,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5565,
                "text": "بَلِ الْإِنْسَانُ عَلَىٰ نَفْسِهِ بَصِيرَةٌ",
                "numberInSurah": 14,
                "juz": 29,
                "manzil": 7,
                "page": 577,
                "ruku": 512,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5566,
                "text": "وَلَوْ أَلْقَىٰ مَعَاذِيرَهُ",
                "numberInSurah": 15,
                "juz": 29,
                "manzil": 7,
                "page": 577,
                "ruku": 512,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5567,
                "text": "لَا تُحَرِّكْ بِهِ لِسَانَكَ لِتَعْجَلَ بِهِ",
                "numberInSurah": 16,
                "juz": 29,
                "manzil": 7,
                "page": 577,
                "ruku": 512,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5568,
                "text": "إِنَّ عَلَيْنَا جَمْعَهُ وَقُرْآنَهُ",
                "numberInSurah": 17,
                "juz": 29,
                "manzil": 7,
                "page": 577,
                "ruku": 512,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5569,
                "text": "فَإِذَا قَرَأْنَاهُ فَاتَّبِعْ قُرْآنَهُ",
                "numberInSurah": 18,
                "juz": 29,
                "manzil": 7,
                "page": 577,
                "ruku": 512,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5570,
                "text": "ثُمَّ إِنَّ عَلَيْنَا بَيَانَهُ",
                "numberInSurah": 19,
                "juz": 29,
                "manzil": 7,
                "page": 577,
                "ruku": 512,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5571,
                "text": "كَلَّا بَلْ تُحِبُّونَ الْعَاجِلَةَ",
                "numberInSurah": 20,
                "juz": 29,
                "manzil": 7,
                "page": 578,
                "ruku": 512,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5572,
                "text": "وَتَذَرُونَ الْآخِرَةَ",
                "numberInSurah": 21,
                "juz": 29,
                "manzil": 7,
                "page": 578,
                "ruku": 512,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5573,
                "text": "وُجُوهٌ يَوْمَئِذٍ نَاضِرَةٌ",
                "numberInSurah": 22,
                "juz": 29,
                "manzil": 7,
                "page": 578,
                "ruku": 512,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5574,
                "text": "إِلَىٰ رَبِّهَا نَاظِرَةٌ",
                "numberInSurah": 23,
                "juz": 29,
                "manzil": 7,
                "page": 578,
                "ruku": 512,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5575,
                "text": "وَوُجُوهٌ يَوْمَئِذٍ بَاسِرَةٌ",
                "numberInSurah": 24,
                "juz": 29,
                "manzil": 7,
                "page": 578,
                "ruku": 512,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5576,
                "text": "تَظُنُّ أَنْ يُفْعَلَ بِهَا فَاقِرَةٌ",
                "numberInSurah": 25,
                "juz": 29,
                "manzil": 7,
                "page": 578,
                "ruku": 512,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5577,
                "text": "كَلَّا إِذَا بَلَغَتِ التَّرَاقِيَ",
                "numberInSurah": 26,
                "juz": 29,
                "manzil": 7,
                "page": 578,
                "ruku": 512,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5578,
                "text": "وَقِيلَ مَنْ ۜ رَاقٍ",
                "numberInSurah": 27,
                "juz": 29,
                "manzil": 7,
                "page": 578,
                "ruku": 512,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5579,
                "text": "وَظَنَّ أَنَّهُ الْفِرَاقُ",
                "numberInSurah": 28,
                "juz": 29,
                "manzil": 7,
                "page": 578,
                "ruku": 512,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5580,
                "text": "وَالْتَفَّتِ السَّاقُ بِالسَّاقِ",
                "numberInSurah": 29,
                "juz": 29,
                "manzil": 7,
                "page": 578,
                "ruku": 512,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5581,
                "text": "إِلَىٰ رَبِّكَ يَوْمَئِذٍ الْمَسَاقُ",
                "numberInSurah": 30,
                "juz": 29,
                "manzil": 7,
                "page": 578,
                "ruku": 512,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5582,
                "text": "فَلَا صَدَّقَ وَلَا صَلَّىٰ",
                "numberInSurah": 31,
                "juz": 29,
                "manzil": 7,
                "page": 578,
                "ruku": 513,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5583,
                "text": "وَلَٰكِنْ كَذَّبَ وَتَوَلَّىٰ",
                "numberInSurah": 32,
                "juz": 29,
                "manzil": 7,
                "page": 578,
                "ruku": 513,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5584,
                "text": "ثُمَّ ذَهَبَ إِلَىٰ أَهْلِهِ يَتَمَطَّىٰ",
                "numberInSurah": 33,
                "juz": 29,
                "manzil": 7,
                "page": 578,
                "ruku": 513,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5585,
                "text": "أَوْلَىٰ لَكَ فَأَوْلَىٰ",
                "numberInSurah": 34,
                "juz": 29,
                "manzil": 7,
                "page": 578,
                "ruku": 513,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5586,
                "text": "ثُمَّ أَوْلَىٰ لَكَ فَأَوْلَىٰ",
                "numberInSurah": 35,
                "juz": 29,
                "manzil": 7,
                "page": 578,
                "ruku": 513,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5587,
                "text": "أَيَحْسَبُ الْإِنْسَانُ أَنْ يُتْرَكَ سُدًى",
                "numberInSurah": 36,
                "juz": 29,
                "manzil": 7,
                "page": 578,
                "ruku": 513,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5588,
                "text": "أَلَمْ يَكُ نُطْفَةً مِنْ مَنِيٍّ يُمْنَىٰ",
                "numberInSurah": 37,
                "juz": 29,
                "manzil": 7,
                "page": 578,
                "ruku": 513,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5589,
                "text": "ثُمَّ كَانَ عَلَقَةً فَخَلَقَ فَسَوَّىٰ",
                "numberInSurah": 38,
                "juz": 29,
                "manzil": 7,
                "page": 578,
                "ruku": 513,
                "hizbQuarter": 231,
                "sajda": false
            },
            {
                "number": 5590,
                "text": "فَجَعَلَ مِنْهُ الزَّوْجَيْنِ الذَّكَرَ وَالْأُنْثَىٰ",
                "numberInSurah": 39,
                "juz": 29,
                "manzil": 7,
                "page": 578,
                "ruku": 513,
                "hizbQuarter": 231,
                "sajda": false
            },
