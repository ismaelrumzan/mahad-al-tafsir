                "juz": 25,
                "manzil": 6,
                "page": 498,
                "ruku": 433,
                "hizbQuarter": 199,
                "sajda": false
            },
            {
                "number": 4469,
                "text": "يَدْعُونَ فِيهَا بِكُلِّ فَاكِهَةٍ آمِنِينَ",
                "numberInSurah": 55,
                "juz": 25,
                "manzil": 6,
                "page": 498,
                "ruku": 433,
                "hizbQuarter": 199,
                "sajda": false
            },
            {
                "number": 4470,
                "text": "لَا يَذُوقُونَ فِيهَا الْمَوْتَ إِلَّا الْمَوْتَةَ الْأُولَىٰ ۖ وَوَقَاهُمْ عَذَابَ الْجَحِيمِ",
                "numberInSurah": 56,
                "juz": 25,
                "manzil": 6,
                "page": 498,
                "ruku": 433,
                "hizbQuarter": 199,
                "sajda": false
            },
            {
                "number": 4471,
                "text": "فَضْلًا مِنْ رَبِّكَ ۚ ذَٰلِكَ هُوَ الْفَوْزُ الْعَظِيمُ",
                "numberInSurah": 57,
                "juz": 25,
                "manzil": 6,
                "page": 498,
                "ruku": 433,
                "hizbQuarter": 199,
                "sajda": false
            },
            {
