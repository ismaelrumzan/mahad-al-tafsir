    "title": "سورة النجم تفسير النسفي",
    "nav": "",
    "id": "4_2_53",
    "verses": {
        "number": 53,
        "name": "سورة النجم",
        "englishName": "An-Najm",
        "englishNameTranslation": "The Star",
        "revelationType": "Meccan",
        "numberOfAyahs": 62,
        "ayahs": [
            {
                "number": 4785,
                "text": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ وَالنَّجْمِ إِذَا هَوَىٰ",
                "numberInSurah": 1,
                "juz": 27,
                "manzil": 7,
                "page": 526,
                "ruku": 460,
                "hizbQuarter": 210,
                "sajda": false
            },
            {
                "number": 4786,
                "text": "مَا ضَلَّ صَاحِبُكُمْ وَمَا غَوَىٰ",
                "numberInSurah": 2,
                "juz": 27,
                "manzil": 7,
                "page": 526,
                "ruku": 460,
                "hizbQuarter": 210,
                "sajda": false
            },
            {
                "number": 4787,
                "text": "وَمَا يَنْطِقُ عَنِ الْهَوَىٰ",
                "numberInSurah": 3,
                "juz": 27,
                "manzil": 7,
                "page": 526,
                "ruku": 460,
                "hizbQuarter": 210,
                "sajda": false
            },
            {
                "number": 4788,
                "text": "إِنْ هُوَ إِلَّا وَحْيٌ يُوحَىٰ",
                "numberInSurah": 4,
                "juz": 27,
                "manzil": 7,
                "page": 526,
                "ruku": 460,
                "hizbQuarter": 210,
                "sajda": false
            },
            {
                "number": 4789,
                "text": "عَلَّمَهُ شَدِيدُ الْقُوَىٰ",
                "numberInSurah": 5,
                "juz": 27,
                "manzil": 7,
                "page": 526,
                "ruku": 460,
                "hizbQuarter": 210,
                "sajda": false
            },
            {
                "number": 4790,
                "text": "ذُو مِرَّةٍ فَاسْتَوَىٰ",
                "numberInSurah": 6,
                "juz": 27,
                "manzil": 7,
                "page": 526,
                "ruku": 460,
                "hizbQuarter": 210,
                "sajda": false
            },
            {
                "number": 4791,
                "text": "وَهُوَ بِالْأُفُقِ الْأَعْلَىٰ",
                "numberInSurah": 7,
                "juz": 27,
                "manzil": 7,
                "page": 526,
                "ruku": 460,
                "hizbQuarter": 210,
                "sajda": false
            },
            {
                "number": 4792,
                "text": "ثُمَّ دَنَا فَتَدَلَّىٰ",
                "numberInSurah": 8,
                "juz": 27,
                "manzil": 7,
                "page": 526,
                "ruku": 460,
                "hizbQuarter": 210,
                "sajda": false
            },
            {
                "number": 4793,
                "text": "فَكَانَ قَابَ قَوْسَيْنِ أَوْ أَدْنَىٰ",
                "numberInSurah": 9,
                "juz": 27,
                "manzil": 7,
                "page": 526,
                "ruku": 460,
                "hizbQuarter": 210,
                "sajda": false
            },
            {
                "number": 4794,
                "text": "فَأَوْحَىٰ إِلَىٰ عَبْدِهِ مَا أَوْحَىٰ",
                "numberInSurah": 10,
                "juz": 27,
                "manzil": 7,
                "page": 526,
                "ruku": 460,
                "hizbQuarter": 210,
                "sajda": false
            },
            {
                "number": 4795,
                "text": "مَا كَذَبَ الْفُؤَادُ مَا رَأَىٰ",
                "numberInSurah": 11,
                "juz": 27,
                "manzil": 7,
                "page": 526,
                "ruku": 460,
                "hizbQuarter": 210,
                "sajda": false
            },
            {
                "number": 4796,
                "text": "أَفَتُمَارُونَهُ عَلَىٰ مَا يَرَىٰ",
                "numberInSurah": 12,
                "juz": 27,
                "manzil": 7,
                "page": 526,
                "ruku": 460,
                "hizbQuarter": 210,
                "sajda": false
            },
            {
                "number": 4797,
                "text": "وَلَقَدْ رَآهُ نَزْلَةً أُخْرَىٰ",
                "numberInSurah": 13,
                "juz": 27,
                "manzil": 7,
                "page": 526,
                "ruku": 460,
                "hizbQuarter": 210,
                "sajda": false
            },
            {
                "number": 4798,
                "text": "عِنْدَ سِدْرَةِ الْمُنْتَهَىٰ",
                "numberInSurah": 14,
                "juz": 27,
                "manzil": 7,
                "page": 526,
                "ruku": 460,
                "hizbQuarter": 210,
                "sajda": false
            },
            {
                "number": 4799,
                "text": "عِنْدَهَا جَنَّةُ الْمَأْوَىٰ",
                "numberInSurah": 15,
                "juz": 27,
                "manzil": 7,
                "page": 526,
                "ruku": 460,
                "hizbQuarter": 210,
                "sajda": false
            },
            {
                "number": 4800,
                "text": "إِذْ يَغْشَى السِّدْرَةَ مَا يَغْشَىٰ",
                "numberInSurah": 16,
                "juz": 27,
                "manzil": 7,
                "page": 526,
                "ruku": 460,
                "hizbQuarter": 210,
                "sajda": false
            },
            {
                "number": 4801,
                "text": "مَا زَاغَ الْبَصَرُ وَمَا طَغَىٰ",
                "numberInSurah": 17,
                "juz": 27,
                "manzil": 7,
                "page": 526,
                "ruku": 460,
                "hizbQuarter": 210,
                "sajda": false
            },
            {
                "number": 4802,
                "text": "لَقَدْ رَأَىٰ مِنْ آيَاتِ رَبِّهِ الْكُبْرَىٰ",
                "numberInSurah": 18,
                "juz": 27,
                "manzil": 7,
                "page": 526,
                "ruku": 460,
                "hizbQuarter": 210,
                "sajda": false
            },
            {
                "number": 4803,
                "text": "أَفَرَأَيْتُمُ اللَّاتَ وَالْعُزَّىٰ",
                "numberInSurah": 19,
                "juz": 27,
                "manzil": 7,
                "page": 526,
                "ruku": 460,
                "hizbQuarter": 210,
                "sajda": false
            },
            {
                "number": 4804,
                "text": "وَمَنَاةَ الثَّالِثَةَ الْأُخْرَىٰ",
                "numberInSurah": 20,
                "juz": 27,
                "manzil": 7,
                "page": 526,
                "ruku": 460,
                "hizbQuarter": 210,
                "sajda": false
            },
            {
                "number": 4805,
                "text": "أَلَكُمُ الذَّكَرُ وَلَهُ الْأُنْثَىٰ",
                "numberInSurah": 21,
                "juz": 27,
                "manzil": 7,
                "page": 526,
                "ruku": 460,
                "hizbQuarter": 210,
                "sajda": false
            },
            {
                "number": 4806,
                "text": "تِلْكَ إِذًا قِسْمَةٌ ضِيزَىٰ",
                "numberInSurah": 22,
                "juz": 27,
                "manzil": 7,
                "page": 526,
                "ruku": 460,
                "hizbQuarter": 210,
                "sajda": false
            },
            {
                "number": 4807,
                "text": "إِنْ هِيَ إِلَّا أَسْمَاءٌ سَمَّيْتُمُوهَا أَنْتُمْ وَآبَاؤُكُمْ مَا أَنْزَلَ اللَّهُ بِهَا مِنْ سُلْطَانٍ ۚ إِنْ يَتَّبِعُونَ إِلَّا الظَّنَّ وَمَا تَهْوَى الْأَنْفُسُ ۖ وَلَقَدْ جَاءَهُمْ مِنْ رَبِّهِمُ الْهُدَىٰ",
                "numberInSurah": 23,
                "juz": 27,
                "manzil": 7,
                "page": 526,
                "ruku": 460,
                "hizbQuarter": 210,
                "sajda": false
            },
            {
                "number": 4808,
                "text": "أَمْ لِلْإِنْسَانِ مَا تَمَنَّىٰ",
                "numberInSurah": 24,
                "juz": 27,
                "manzil": 7,
                "page": 526,
                "ruku": 460,
                "hizbQuarter": 210,
                "sajda": false
            },
            {
                "number": 4809,
                "text": "فَلِلَّهِ الْآخِرَةُ وَالْأُولَىٰ",
                "numberInSurah": 25,
                "juz": 27,
                "manzil": 7,
                "page": 526,
                "ruku": 460,
                "hizbQuarter": 210,
                "sajda": false
            },
            {
                "number": 4810,
                "text": "۞ وَكَمْ مِنْ مَلَكٍ فِي السَّمَاوَاتِ لَا تُغْنِي شَفَاعَتُهُمْ شَيْئًا إِلَّا مِنْ بَعْدِ أَنْ يَأْذَنَ اللَّهُ لِمَنْ يَشَاءُ وَيَرْضَىٰ",
                "numberInSurah": 26,
                "juz": 27,
                "manzil": 7,
                "page": 526,
                "ruku": 461,
                "hizbQuarter": 211,
                "sajda": false
            },
            {
                "number": 4811,
                "text": "إِنَّ الَّذِينَ لَا يُؤْمِنُونَ بِالْآخِرَةِ لَيُسَمُّونَ الْمَلَائِكَةَ تَسْمِيَةَ الْأُنْثَىٰ",
                "numberInSurah": 27,
                "juz": 27,
                "manzil": 7,
                "page": 527,
                "ruku": 461,
                "hizbQuarter": 211,
                "sajda": false
            },
            {
                "number": 4812,
                "text": "وَمَا لَهُمْ بِهِ مِنْ عِلْمٍ ۖ إِنْ يَتَّبِعُونَ إِلَّا الظَّنَّ ۖ وَإِنَّ الظَّنَّ لَا يُغْنِي مِنَ الْحَقِّ شَيْئًا",
                "numberInSurah": 28,
                "juz": 27,
                "manzil": 7,
                "page": 527,
                "ruku": 461,
                "hizbQuarter": 211,
                "sajda": false
            },
            {
                "number": 4813,
                "text": "فَأَعْرِضْ عَنْ مَنْ تَوَلَّىٰ عَنْ ذِكْرِنَا وَلَمْ يُرِدْ إِلَّا الْحَيَاةَ الدُّنْيَا",
                "numberInSurah": 29,
