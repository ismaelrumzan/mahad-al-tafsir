    "verses": {
        "number": 69,
        "name": "سورة الحاقة",
        "englishName": "Al-Haaqqa",
        "englishNameTranslation": "The Reality",
        "revelationType": "Meccan",
        "numberOfAyahs": 52,
        "ayahs": [
            {
                "number": 5324,
                "text": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ الْحَاقَّةُ",
                "numberInSurah": 1,
                "juz": 29,
                "manzil": 7,
                "page": 566,
                "ruku": 500,
                "hizbQuarter": 227,
                "sajda": false
            },
            {
                "number": 5325,
                "text": "مَا الْحَاقَّةُ",
                "numberInSurah": 2,
                "juz": 29,
                "manzil": 7,
                "page": 566,
                "ruku": 500,
                "hizbQuarter": 227,
                "sajda": false
            },
            {
                "number": 5326,
                "text": "وَمَا أَدْرَاكَ مَا الْحَاقَّةُ",
                "numberInSurah": 3,
                "juz": 29,
                "manzil": 7,
                "page": 566,
                "ruku": 500,
                "hizbQuarter": 227,
                "sajda": false
            },
            {
                "number": 5327,
                "text": "كَذَّبَتْ ثَمُودُ وَعَادٌ بِالْقَارِعَةِ",
                "numberInSurah": 4,
                "juz": 29,
                "manzil": 7,
                "page": 566,
                "ruku": 500,
                "hizbQuarter": 227,
                "sajda": false
            },
            {
                "number": 5328,
                "text": "فَأَمَّا ثَمُودُ فَأُهْلِكُوا بِالطَّاغِيَةِ",
                "numberInSurah": 5,
                "juz": 29,
                "manzil": 7,
                "page": 566,
                "ruku": 500,
                "hizbQuarter": 227,
                "sajda": false
            },
            {
                "number": 5329,
                "text": "وَأَمَّا عَادٌ فَأُهْلِكُوا بِرِيحٍ صَرْصَرٍ عَاتِيَةٍ",
                "numberInSurah": 6,
                "juz": 29,
                "manzil": 7,
                "page": 566,
                "ruku": 500,
                "hizbQuarter": 227,
                "sajda": false
            },
            {
                "number": 5330,
                "text": "سَخَّرَهَا عَلَيْهِمْ سَبْعَ لَيَالٍ وَثَمَانِيَةَ أَيَّامٍ حُسُومًا فَتَرَى الْقَوْمَ فِيهَا صَرْعَىٰ كَأَنَّهُمْ أَعْجَازُ نَخْلٍ خَاوِيَةٍ",
                "numberInSurah": 7,
                "juz": 29,
                "manzil": 7,
                "page": 566,
                "ruku": 500,
                "hizbQuarter": 227,
                "sajda": false
            },
            {
                "number": 5331,
                "text": "فَهَلْ تَرَىٰ لَهُمْ مِنْ بَاقِيَةٍ",
                "numberInSurah": 8,
                "juz": 29,
                "manzil": 7,
                "page": 566,
                "ruku": 500,
                "hizbQuarter": 227,
                "sajda": false
            },
            {
                "number": 5332,
                "text": "وَجَاءَ فِرْعَوْنُ وَمَنْ قَبْلَهُ وَالْمُؤْتَفِكَاتُ بِالْخَاطِئَةِ",
                "numberInSurah": 9,
                "juz": 29,
                "manzil": 7,
                "page": 567,
                "ruku": 500,
                "hizbQuarter": 227,
                "sajda": false
            },
            {
                "number": 5333,
                "text": "فَعَصَوْا رَسُولَ رَبِّهِمْ فَأَخَذَهُمْ أَخْذَةً رَابِيَةً",
                "numberInSurah": 10,
                "juz": 29,
                "manzil": 7,
                "page": 567,
                "ruku": 500,
                "hizbQuarter": 227,
                "sajda": false
            },
            {
                "number": 5334,
                "text": "إِنَّا لَمَّا طَغَى الْمَاءُ حَمَلْنَاكُمْ فِي الْجَارِيَةِ",
                "numberInSurah": 11,
                "juz": 29,
                "manzil": 7,
                "page": 567,
                "ruku": 500,
                "hizbQuarter": 227,
                "sajda": false
            },
            {
                "number": 5335,
                "text": "لِنَجْعَلَهَا لَكُمْ تَذْكِرَةً وَتَعِيَهَا أُذُنٌ وَاعِيَةٌ",
                "numberInSurah": 12,
                "juz": 29,
                "manzil": 7,
                "page": 567,
                "ruku": 500,
                "hizbQuarter": 227,
                "sajda": false
            },
            {
                "number": 5336,
                "text": "فَإِذَا نُفِخَ فِي الصُّورِ نَفْخَةٌ وَاحِدَةٌ",
                "numberInSurah": 13,
                "juz": 29,
                "manzil": 7,
                "page": 567,
                "ruku": 500,
                "hizbQuarter": 227,
                "sajda": false
            },
            {
                "number": 5337,
                "text": "وَحُمِلَتِ الْأَرْضُ وَالْجِبَالُ فَدُكَّتَا دَكَّةً وَاحِدَةً",
                "numberInSurah": 14,
                "juz": 29,
                "manzil": 7,
                "page": 567,
                "ruku": 500,
                "hizbQuarter": 227,
                "sajda": false
            },
            {
                "number": 5338,
                "text": "فَيَوْمَئِذٍ وَقَعَتِ الْوَاقِعَةُ",
                "numberInSurah": 15,
                "juz": 29,
                "manzil": 7,
                "page": 567,
                "ruku": 500,
                "hizbQuarter": 227,
                "sajda": false
            },
            {
                "number": 5339,
                "text": "وَانْشَقَّتِ السَّمَاءُ فَهِيَ يَوْمَئِذٍ وَاهِيَةٌ",
                "numberInSurah": 16,
                "juz": 29,
                "manzil": 7,
                "page": 567,
                "ruku": 500,
                "hizbQuarter": 227,
                "sajda": false
            },
            {
                "number": 5340,
                "text": "وَالْمَلَكُ عَلَىٰ أَرْجَائِهَا ۚ وَيَحْمِلُ عَرْشَ رَبِّكَ فَوْقَهُمْ يَوْمَئِذٍ ثَمَانِيَةٌ",
                "numberInSurah": 17,
                "juz": 29,
                "manzil": 7,
                "page": 567,
                "ruku": 500,
                "hizbQuarter": 227,
                "sajda": false
            },
            {
                "number": 5341,
                "text": "يَوْمَئِذٍ تُعْرَضُونَ لَا تَخْفَىٰ مِنْكُمْ خَافِيَةٌ",
                "numberInSurah": 18,
                "juz": 29,
                "manzil": 7,
                "page": 567,
                "ruku": 500,
                "hizbQuarter": 227,
                "sajda": false
            },
            {
                "number": 5342,
                "text": "فَأَمَّا مَنْ أُوتِيَ كِتَابَهُ بِيَمِينِهِ فَيَقُولُ هَاؤُمُ اقْرَءُوا كِتَابِيَهْ",
                "numberInSurah": 19,
                "juz": 29,
                "manzil": 7,
                "page": 567,
                "ruku": 500,
                "hizbQuarter": 227,
                "sajda": false
            },
            {
                "number": 5343,
                "text": "إِنِّي ظَنَنْتُ أَنِّي مُلَاقٍ حِسَابِيَهْ",
                "numberInSurah": 20,
                "juz": 29,
                "manzil": 7,
                "page": 567,
                "ruku": 500,
                "hizbQuarter": 227,
                "sajda": false
            },
            {
                "number": 5344,
                "text": "فَهُوَ فِي عِيشَةٍ رَاضِيَةٍ",
                "numberInSurah": 21,
                "juz": 29,
                "manzil": 7,
                "page": 567,
                "ruku": 500,
                "hizbQuarter": 227,
                "sajda": false
            },
            {
                "number": 5345,
                "text": "فِي جَنَّةٍ عَالِيَةٍ",
                "numberInSurah": 22,
                "juz": 29,
                "manzil": 7,
                "page": 567,
                "ruku": 500,
                "hizbQuarter": 227,
                "sajda": false
            },
            {
                "number": 5346,
                "text": "قُطُوفُهَا دَانِيَةٌ",
                "numberInSurah": 23,
                "juz": 29,
