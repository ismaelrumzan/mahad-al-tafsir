    "verses": {
        "number": 71,
        "name": "سورة نوح",
        "englishName": "Nooh",
        "englishNameTranslation": "Noah",
        "revelationType": "Meccan",
        "numberOfAyahs": 28,
        "ayahs": [
            {
                "number": 5420,
                "text": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ إِنَّا أَرْسَلْنَا نُوحًا إِلَىٰ قَوْمِهِ أَنْ أَنْذِرْ قَوْمَكَ مِنْ قَبْلِ أَنْ يَأْتِيَهُمْ عَذَابٌ أَلِيمٌ",
                "numberInSurah": 1,
                "juz": 29,
                "manzil": 7,
                "page": 570,
                "ruku": 504,
                "hizbQuarter": 228,
                "sajda": false
            },
            {
                "number": 5421,
                "text": "قَالَ يَا قَوْمِ إِنِّي لَكُمْ نَذِيرٌ مُبِينٌ",
                "numberInSurah": 2,
                "juz": 29,
                "manzil": 7,
                "page": 570,
                "ruku": 504,
                "hizbQuarter": 228,
                "sajda": false
            },
            {
                "number": 5422,
                "text": "أَنِ اعْبُدُوا اللَّهَ وَاتَّقُوهُ وَأَطِيعُونِ",
                "numberInSurah": 3,
                "juz": 29,
                "manzil": 7,
                "page": 570,
                "ruku": 504,
                "hizbQuarter": 228,
                "sajda": false
            },
            {
                "number": 5423,
                "text": "يَغْفِرْ لَكُمْ مِنْ ذُنُوبِكُمْ وَيُؤَخِّرْكُمْ إِلَىٰ أَجَلٍ مُسَمًّى ۚ إِنَّ أَجَلَ اللَّهِ إِذَا جَاءَ لَا يُؤَخَّرُ ۖ لَوْ كُنْتُمْ تَعْلَمُونَ",
                "numberInSurah": 4,
                "juz": 29,
                "manzil": 7,
                "page": 570,
                "ruku": 504,
                "hizbQuarter": 228,
                "sajda": false
            },
            {
                "number": 5424,
                "text": "قَالَ رَبِّ إِنِّي دَعَوْتُ قَوْمِي لَيْلًا وَنَهَارًا",
                "numberInSurah": 5,
                "juz": 29,
                "manzil": 7,
                "page": 570,
                "ruku": 504,
                "hizbQuarter": 228,
                "sajda": false
            },
            {
                "number": 5425,
                "text": "فَلَمْ يَزِدْهُمْ دُعَائِي إِلَّا فِرَارًا",
                "numberInSurah": 6,
                "juz": 29,
                "manzil": 7,
                "page": 570,
                "ruku": 504,
                "hizbQuarter": 228,
                "sajda": false
            },
            {
                "number": 5426,
                "text": "وَإِنِّي كُلَّمَا دَعَوْتُهُمْ لِتَغْفِرَ لَهُمْ جَعَلُوا أَصَابِعَهُمْ فِي آذَانِهِمْ وَاسْتَغْشَوْا ثِيَابَهُمْ وَأَصَرُّوا وَاسْتَكْبَرُوا اسْتِكْبَارًا",
                "numberInSurah": 7,
                "juz": 29,
                "manzil": 7,
                "page": 570,
                "ruku": 504,
                "hizbQuarter": 228,
                "sajda": false
            },
            {
                "number": 5427,
                "text": "ثُمَّ إِنِّي دَعَوْتُهُمْ جِهَارًا",
                "numberInSurah": 8,
                "juz": 29,
                "manzil": 7,
                "page": 570,
                "ruku": 504,
                "hizbQuarter": 228,
                "sajda": false
            },
            {
                "number": 5428,
                "text": "ثُمَّ إِنِّي أَعْلَنْتُ لَهُمْ وَأَسْرَرْتُ لَهُمْ إِسْرَارًا",
                "numberInSurah": 9,
                "juz": 29,
                "manzil": 7,
                "page": 570,
                "ruku": 504,
                "hizbQuarter": 228,
                "sajda": false
            },
            {
                "number": 5429,
                "text": "فَقُلْتُ اسْتَغْفِرُوا رَبَّكُمْ إِنَّهُ كَانَ غَفَّارًا",
                "numberInSurah": 10,
                "juz": 29,
                "manzil": 7,
                "page": 570,
                "ruku": 504,
                "hizbQuarter": 228,
                "sajda": false
            },
            {
                "number": 5430,
                "text": "يُرْسِلِ السَّمَاءَ عَلَيْكُمْ مِدْرَارًا",
                "numberInSurah": 11,
                "juz": 29,
                "manzil": 7,
                "page": 571,
                "ruku": 504,
                "hizbQuarter": 228,
                "sajda": false
            },
            {
                "number": 5431,
                "text": "وَيُمْدِدْكُمْ بِأَمْوَالٍ وَبَنِينَ وَيَجْعَلْ لَكُمْ جَنَّاتٍ وَيَجْعَلْ لَكُمْ أَنْهَارًا",
                "numberInSurah": 12,
                "juz": 29,
                "manzil": 7,
                "page": 571,
                "ruku": 504,
                "hizbQuarter": 228,
                "sajda": false
            },
            {
                "number": 5432,
                "text": "مَا لَكُمْ لَا تَرْجُونَ لِلَّهِ وَقَارًا",
                "numberInSurah": 13,
                "juz": 29,
                "manzil": 7,
                "page": 571,
                "ruku": 504,
                "hizbQuarter": 228,
                "sajda": false
            },
            {
                "number": 5433,
                "text": "وَقَدْ خَلَقَكُمْ أَطْوَارًا",
                "numberInSurah": 14,
                "juz": 29,
                "manzil": 7,
                "page": 571,
                "ruku": 504,
                "hizbQuarter": 228,
                "sajda": false
            },
            {
                "number": 5434,
                "text": "أَلَمْ تَرَوْا كَيْفَ خَلَقَ اللَّهُ سَبْعَ سَمَاوَاتٍ طِبَاقًا",
                "numberInSurah": 15,
                "juz": 29,
                "manzil": 7,
                "page": 571,
                "ruku": 504,
                "hizbQuarter": 228,
                "sajda": false
            },
            {
                "number": 5435,
                "text": "وَجَعَلَ الْقَمَرَ فِيهِنَّ نُورًا وَجَعَلَ الشَّمْسَ سِرَاجًا",
                "numberInSurah": 16,
                "juz": 29,
                "manzil": 7,
                "page": 571,
                "ruku": 504,
                "hizbQuarter": 228,
                "sajda": false
            },
            {
                "number": 5436,
                "text": "وَاللَّهُ أَنْبَتَكُمْ مِنَ الْأَرْضِ نَبَاتًا",
                "numberInSurah": 17,
                "juz": 29,
                "manzil": 7,
                "page": 571,
                "ruku": 504,
                "hizbQuarter": 228,
                "sajda": false
            },
            {
                "number": 5437,
                "text": "ثُمَّ يُعِيدُكُمْ فِيهَا وَيُخْرِجُكُمْ إِخْرَاجًا",
                "numberInSurah": 18,
                "juz": 29,
                "manzil": 7,
                "page": 571,
                "ruku": 504,
                "hizbQuarter": 228,
                "sajda": false
            },
            {
                "number": 5438,
                "text": "وَاللَّهُ جَعَلَ لَكُمُ الْأَرْضَ بِسَاطًا",
                "numberInSurah": 19,
                "juz": 29,
                "manzil": 7,
                "page": 571,
                "ruku": 504,
                "hizbQuarter": 228,
                "sajda": false
            },
            {
                "number": 5439,
                "text": "لِتَسْلُكُوا مِنْهَا سُبُلًا فِجَاجًا",
                "numberInSurah": 20,
                "juz": 29,
                "manzil": 7,
                "page": 571,
                "ruku": 504,
                "hizbQuarter": 228,
                "sajda": false
            },
            {
                "number": 5440,
                "text": "قَالَ نُوحٌ رَبِّ إِنَّهُمْ عَصَوْنِي وَاتَّبَعُوا مَنْ لَمْ يَزِدْهُ مَالُهُ وَوَلَدُهُ إِلَّا خَسَارًا",
                "numberInSurah": 21,
                "juz": 29,
                "manzil": 7,
                "page": 571,
                "ruku": 505,
                "hizbQuarter": 228,
                "sajda": false
            },
            {
                "number": 5441,
                "text": "وَمَكَرُوا مَكْرًا كُبَّارًا",
                "numberInSurah": 22,
                "juz": 29,
                "manzil": 7,
                "page": 571,
                "ruku": 505,
                "hizbQuarter": 228,
                "sajda": false
            },
            {
                "number": 5442,
                "text": "وَقَالُوا لَا تَذَرُنَّ آلِهَتَكُمْ وَلَا تَذَرُنَّ وَدًّا وَلَا سُوَاعًا وَلَا يَغُوثَ وَيَعُوقَ وَنَسْرًا",
                "numberInSurah": 23,
                "juz": 29,
                "manzil": 7,
                "page": 571,
                "ruku": 505,
                "hizbQuarter": 228,
                "sajda": false
            },
            {
                "number": 5443,
                "text": "وَقَدْ أَضَلُّوا كَثِيرًا ۖ وَلَا تَزِدِ الظَّالِمِينَ إِلَّا ضَلَالًا",
                "numberInSurah": 24,
                "juz": 29,
                "manzil": 7,
                "page": 571,
                "ruku": 505,
                "hizbQuarter": 228,
                "sajda": false
            },
            {
                "number": 5444,
                "text": "مِمَّا خَطِيئَاتِهِمْ أُغْرِقُوا فَأُدْخِلُوا نَارًا فَلَمْ يَجِدُوا لَهُمْ مِنْ دُونِ اللَّهِ أَنْصَارًا",
                "numberInSurah": 25,
                "juz": 29,
                "manzil": 7,
                "page": 571,
                "ruku": 505,
                "hizbQuarter": 228,
                "sajda": false
            },
            {
                "number": 5445,
                "text": "وَقَالَ نُوحٌ رَبِّ لَا تَذَرْ عَلَى الْأَرْضِ مِنَ الْكَافِرِينَ دَيَّارًا",
                "numberInSurah": 26,
                "juz": 29,
                "manzil": 7,
                "page": 571,
                "ruku": 505,
                "hizbQuarter": 228,
                "sajda": false
            },
            {
                "number": 5446,
                "text": "إِنَّكَ إِنْ تَذَرْهُمْ يُضِلُّوا عِبَادَكَ وَلَا يَلِدُوا إِلَّا فَاجِرًا كَفَّارًا",
                "numberInSurah": 27,
                "juz": 29,
                "manzil": 7,
                "page": 571,
                "ruku": 505,
                "hizbQuarter": 228,
                "sajda": false
            },
            {
                "number": 5447,
                "text": "رَبِّ اغْفِرْ لِي وَلِوَالِدَيَّ وَلِمَنْ دَخَلَ بَيْتِيَ مُؤْمِنًا وَلِلْمُؤْمِنِينَ وَالْمُؤْمِنَاتِ وَلَا تَزِدِ الظَّالِمِينَ إِلَّا تَبَارًا",
                "numberInSurah": 28,
                "juz": 29,
                "manzil": 7,
                "page": 571,
                "ruku": 505,
                "hizbQuarter": 228,
                "sajda": false
            }
